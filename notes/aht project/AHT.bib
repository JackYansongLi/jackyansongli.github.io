}

@misc{zhong_gec_2023,
  title = {{{GEC}}: {{A Unified Framework}} for {{Interactive Decision Making}} in {{MDP}}, {{POMDP}}, and {{Beyond}}},
  shorttitle = {{{GEC}}},
